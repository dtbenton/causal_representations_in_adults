vec_1 = sample_y[data_2==x]
vec_2 = sample_y[data_2==y]
dif = vec_1 - vec_2
c[i] = mean(dif)
}
bb_dif = mean(data_1[data_2==x]-data_1[data_2==y])
c(mean(c),bb_dif,sum(abs(c) > bb_dif)/length(c),sum(abs(c) < bb_dif)/length(c),
mean(data_1[data_2==x]),mean(data_1[data_2==y]))
}
